{
    "name": "dazzlesoftware/string",
    "type": "library",
    "description": "Dazzle String Package",
    "keywords": ["dazzle", "framework", "string"],
    "homepage": "https://github.com/dazzle-framework/string",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0"
    },
    "require-dev": {
        "doctrine/inflector": "^1.2",
        "dazzlesoftware/dazzle-coding-standard": "^1.0|^2.0",
        "dazzlesoftware/test": "^1.0|^2.0",
        "phpunit/phpunit": "^9.0"
    },
    "conflict": {
        "doctrine/inflector": "<1.2"
    },
    "suggest": {
        "ext-mbstring": "For improved processing",
        "doctrine/inflector": "To use the string inflector"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\String\\": "src/"
        },
        "files": [
            "src/phputf8/utf8.php",
            "src/phputf8/ord.php",
            "src/phputf8/str_ireplace.php",
            "src/phputf8/str_pad.php",
            "src/phputf8/str_split.php",
            "src/phputf8/strcasecmp.php",
            "src/phputf8/strcspn.php",
            "src/phputf8/stristr.php",
            "src/phputf8/strrev.php",
            "src/phputf8/strspn.php",
            "src/phputf8/trim.php",
            "src/phputf8/ucfirst.php",
            "src/phputf8/ucwords.php",
            "src/phputf8/utils/ascii.php",
            "src/phputf8/utils/validation.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\String\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-develop": "2.x-dev"
        }
    }
}
